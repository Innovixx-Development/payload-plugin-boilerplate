{
	"name": "payload Plugin Boilerplate",
	"dockerComposeFile": "docker-compose.yml",
	"service": "demo",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"customizations": {
		"vscode": {
			"extensions": [
				"mongodb.mongodb-vscode"
			]
		}
	},
	"forwardPorts": [
		3000,
		27017
	],
	"postCreateCommand": "yarn install"
}